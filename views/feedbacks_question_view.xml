<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="feedbacks_question_tree_view_id" model="ir.ui.view">
        <field name="name">feedbacks_tree_question_view_id</field>
        <field name="model">feedbacks.questions</field>
        <field name="arch" type="xml">
            <tree>
                <field name="questionId"/>
                <field name="questionText"/>
            </tree>
        </field>
    </record>

    <record id="feedbacks_question_form_view_id" model="ir.ui.view">
        <field name="name">feedbacks_question_form_view_id</field>
        <field name="model">feedbacks.questions</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="questionId" readonly="1"/>
                        </h1>
                    </div>
                    <group>
                        <group>
                            <field name="questionText"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
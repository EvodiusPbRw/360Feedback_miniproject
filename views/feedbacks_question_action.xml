<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="question_action_id" model="ir.actions.act_window">
        <field name="name">Questions</field>
        <field name="res_model">feedbacks.questions</field>
    </record>

    <record id="feedbacks_question_actions_tree_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="3"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="feedbacks_360.feedbacks_question_tree_view_id"/>
        <field name="act_window_id" ref="feedbacks_360.question_action_id"/>
    </record>

    <record id="feedbacks_question_actions_form_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="4"/>
        <field name="view_mode">form</field>
        <field name="view_id" ref="feedbacks_360.feedbacks_question_form_view_id"/>
        <field name="act_window_id" ref="feedbacks_360.question_action_id"/>
    </record>
</odoo>
<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="persons_action_id" model="ir.actions.act_window">
        <field name="name">Persons</field>
        <field name="res_model">feedbacks.persons</field>
    </record>

    <record id="feedbacks_persons_actions_tree_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="3"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="feedbacks_persons_tree_view_id"/>
        <field name="act_window_id" ref="feedbacks_360.persons_action_id"/>
    </record>

    <record id="feedbacks_persons_actions_form_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="4"/>
        <field name="view_mode">form</field>
        <field name="view_id" ref="feedbacks_persons_form_view_id"/>
        <field name="act_window_id" ref="feedbacks_360.persons_action_id"/>
    </record>
</odoo>
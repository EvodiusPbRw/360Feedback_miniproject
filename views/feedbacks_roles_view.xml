<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="feedbacks_roles_tree_view_id" model="ir.ui.view">
        <field name="name">feedbacks_tree_view_id</field>
        <field name="model">feedbacks.roles</field>
        <field name="arch" type="xml">
            <tree>
                <field name="roleId"/>
                <field name="roleName"/>
                <field name="create_date"/>
                <field name="write_date"/>
            </tree>
        </field>
    </record>

    <record id="feedbacks_roles_form_view_id" model="ir.ui.view">
        <field name="name">feedbacks_roles_from_view_id</field>
        <field name="model">feedbacks.roles</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="roleId" readonly="1"/>
                        </h1>
                    </div>
                    <group>
                        <group>
                            <field name="roleName"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
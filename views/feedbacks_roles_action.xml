<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="roles_action_id" model="ir.actions.act_window">
        <field name="name">Roles</field>
        <field name="res_model">feedbacks.roles</field>
    </record>

    <record id="feedbacks_roles_actions_tree_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="1"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="feedbacks_360.feedbacks_roles_tree_view_id"/>
        <field name="act_window_id" ref="feedbacks_360.roles_action_id"/>
    </record>

    <record id="feedbacks_roles_actions_form_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="2"/>
        <field name="view_mode">form</field>
        <field name="view_id" ref="feedbacks_360.feedbacks_roles_form_view_id"/>
        <field name="act_window_id" ref="feedbacks_360.roles_action_id"/>
    </record>
</odoo>
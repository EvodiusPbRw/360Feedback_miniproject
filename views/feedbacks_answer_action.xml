<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="answer_action_id" model="ir.actions.act_window">
        <field name="name">answer</field>
        <field name="res_model">feedbacks.answers</field>
    </record>

    <record id="feedbacks_answer_actions_tree_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="3"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="feedbacks_360.feedbacks_answer_tree_view_id"/>
        <field name="act_window_id" ref="feedbacks_360.answer_action_id"/>
    </record>

    <record id="feedbacks_answer_actions_form_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="4"/>
        <field name="view_mode">form</field>
        <field name="view_id" ref="feedbacks_360.feedbacks_answer_form_view_id"/>
        <field name="act_window_id" ref="feedbacks_360.answer_action_id"/>
    </record>
</odoo>